{
  "name": "stock-insights",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "babel-node server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "babel-node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "babel": "^5.8.23",
    "better-npm-run": "0.0.4",
    "bluebird": "^2.9.34",
    "body-parser": "^1.13.3",
    "classnames": "^2.1.3",
    "clone": "^1.0.2",
    "d3": "^3.5.6",
    "dimple": "git+https://github.com/PMSI-AlignAlytics/dimple.git",
    "express": "^4.13.3",
    "g11n-pipeline": "^1.1.3",
    "keymirror": "^0.1.1",
    "locale": "0.0.20",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "object-assign": "^3.0.0",
    "path": "^0.12.7",
    "react": "^0.14.2",
    "react-bubble-chart": "^0.3.0",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "request": "^2.61.0",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "~5.8.33",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.11.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-logger": "^2.0.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "4.2.1"
  }
}
